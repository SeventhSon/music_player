                                    <Condition Property="IsSelected" Value="true"/>
                                    <Condition Property="Selector.IsSelectionActive" Value="false"/>
                                </MultiTrigger.Conditions>
                                <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.InactiveSelectionHighlightTextBrushKey}}"/>
                                <Setter Property="Background" Value="White"/>
                                <Setter Property="Background" TargetName="Bd" Value="White"/>
                            </MultiTrigger>
                            <Trigger Property="IsEnabled" Value="false">
                                <Setter Property="Foreground" Value="{DynamicResource {x:Static SystemColors.GrayTextBrushKey}}"/>
                            </Trigger>
                        </ControlTemplate.Triggers>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Grid>
        <Grid.Resources>
            <my:IntToTimeConverter x:Key="IntToTimeConverter"/>
        </Grid.Resources>
        <Grid.RowDefinitions>
            <RowDefinition Height="75" />
            <RowDefinition Height="34" />
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="165" />
            <ColumnDefinition MinWidth="100" />
            <ColumnDefinition Width="97" />
        </Grid.ColumnDefinitions>
        <Rectangle Grid.ColumnSpan="3" Margin="0" RadiusY="40" Grid.Row="1" Stroke="#FFADB4FF" Grid.RowSpan="2" Fill="White"/>
        <Rectangle Grid.ColumnSpan="3" Margin="0" RadiusY="40" Stroke="#FFADB4FF" Fill="White"/>
        <Rectangle Grid.ColumnSpan="2" RadiusY="40" Grid.Row="1" Grid.Column="1">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Offset="1"/>
                    <GradientStop Color="#FFADB4FF"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <Button x:Name="NextBtn" Content="Button" HorizontalAlignment="Right" Margin="0,17.667,10,17.333" Width="40" RenderTransformOrigin="0.083,0.15" Style="{DynamicResource PlayBtnStyle}"/>
        <ToggleButton x:Name="PlayPauseTgBtn" IsChecked="{Binding IsPlaying, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Content="ToggleButton" HorizontalAlignment="Center" Margin="57.667,11.833,57.333,13.167" VerticalAlignment="Center" Width="50" Height="50" Style="{DynamicResource PlayPauseBtnStyle}" RenderTransformOrigin="0.5,0.49"/>
        <Button x:Name="PrevBtn" Content="Button" HorizontalAlignment="Left" Margin="10,17.542,0,17.458" Width="40" RenderTransformOrigin="0.083,0.15" Style="{DynamicResource PlayBtnStyle}">
            <Button.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="180"/>
                    <TranslateTransform X="33.5" Y="28"/>
                </TransformGroup>
            </Button.RenderTransform>
        </Button>
        <DataGrid x:Name="SongGrid" CellStyle="{StaticResource Body_Content_DataGrid_Centering}" Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" Background="White" BorderBrush="{x:Null}" AlternatingRowBackground="White" RowBackground="#FFEBECF5" ItemsSource="{Binding Path=Results,UpdateSourceTrigger=PropertyChanged,Mode=TwoWay}" FontFamily="Tw Cen MT" GridLinesVisibility="None" BorderThickness="0" Foreground="#FF7E7EFF" MinColumnWidth="60" RowHeight="25" CanUserAddRows="False" CanUserDeleteRows="False" CanUserResizeRows="False" FontSize="14" IsReadOnly="True">
            <DataGrid.InputBindings>
                <MouseBinding Gesture="LeftDoubleClick" Command="{Binding PlayCommand}" CommandParameter="{Binding ElementName=SongGrid, Path=SelectedIndex}" />
            </DataGrid.InputBindings>
        </DataGrid>
        <Rectangle Grid.Column="2" Fill="#FFADB4FF" HorizontalAlignment="Left" Height="18.75" Margin="54.75,46.25,0,0" RadiusY="40" VerticalAlignment="Top" Width="17.25"/>
        <Rectangle Grid.Column="2" Fill="#FFADB4FF" HorizontalAlignment="Left" Height="6.667" Margin="10,58.333,0,0" RadiusY="40" VerticalAlignment="Top" Width="15"/>
        <Rectangle Grid.Column="2" Fill="#FFADB4FF" HorizontalAlignment="Left" Height="9.25" Margin="25,55.75,0,0" RadiusY="40" VerticalAlignment="Top" Width="15"/>
        <Rectangle Grid.Column="2" Fill="#FFADB4FF" HorizontalAlignment="Left" Height="13.25" Margin="40,51.75,0,0" RadiusY="40" VerticalAlignment="Top" Width="15.021"/>
        <Slider x:Name="TimeSlider" Grid.Column="1" Margin="10,58.333,10,0" Style="{DynamicResource SimpleSlider}" Foreground="#FF7E7EFF" Maximum="100" Value="{Binding PercentagePlayed}">
            <Slider.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="White" Offset="0"/>
                    <GradientStop Color="#FF867AFF" Offset="1"/>
                </LinearGradientBrush>
            </Slider.Background>
            <Slider.BorderBrush>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="White" Offset="0"/>
                    <GradientStop Color="#FF7E7EFF" Offset="1"/>
                </LinearGradientBrush>
            </Slider.BorderBrush>
        </Slider>
        <Slider x:Name="VolumeSlider" Grid.Column="2" Margin="10,58.333,25,1.5" Style="{DynamicResource SimpleSlider}" Foreground="#FF7E7EFF" Value="{Binding Volume, UpdateSourceTrigger=PropertyChanged}" Maximum="100" RenderTransformOrigin="0.486,0.44">
            <Slider.Background>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="White" Offset="0"/>
                    <GradientStop Color="#FF867AFF" Offset="1"/>
                </LinearGradientBrush>
            </Slider.Background>
            <Slider.BorderBrush>
                <LinearGradientBrush EndPoint="0,1" StartPoint="0,0">
                    <GradientStop Color="White" Offset="0"/>
                    <GradientStop Color="#FF7E7EFF" Offset="1"/>
                </LinearGradientBrush>
            </Slider.BorderBrush>
        </Slider>
        <Rectangle RadiusY="40" Grid.Row="1" Grid.RowSpan="2" Stroke="#FFADB4FF">
            <Rectangle.Fill>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FFEBECF5" Offset="1"/>
                    <GradientStop Color="#FFEBECF5"/>
                </LinearGradientBrush>
            </Rectangle.Fill>
        </Rectangle>
        <TextBlock x:Name="ArtistTB" Grid.Column="1" HorizontalAlignment="Left" Height="18" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="217" Foreground="#FF7E7EFF" FontFamily="Tw Cen MT Condensed" FontSize="16"><Run Language="pl-pl" Text="{Binding NowPlayingArtist}"/></TextBlock>
        <TextBlock x:Name="TitleTB" Grid.Column="1" Margin="10,29,103,13" TextWrapping="Wrap" Foreground="#FF7E7EFF" FontWeight="Bold" FontFamily="Tw Cen MT Condensed Extra Bold" FontSize="26.667"><Run Language="pl-pl" Text="{Binding NowPlayingTrack}"/></TextBlock>
        <ListBox HorizontalAlignment="Left" Height="445" Grid.Row="1" VerticalAlignment="Top" Width="165" Grid.RowSpan="2" Background="#00000000" BorderBrush="{x:Null}">
            <ListBoxItem Content="Now Playing" Style="{DynamicResource ListBoxItemStyle1}" FontFamily="Tw Cen MT" FontWeight="Bold" FontSize="21.333" Foreground="#FF7E7EFF" Height="36"/>
            <ListBoxItem IsSelected="True" Content="Library" Style="{DynamicResource ListBoxItemStyle1}" FontFamily="Tw Cen MT" FontWeight="Bold" FontSize="21.333" Foreground="#FF7E7EFF" Height="36"/>
            <ListBoxItem Content="Add" Style="{DynamicResource ListBoxItemStyle1}" FontFamily="Tw Cen MT" FontWeight="Bold" FontSize="21.333" Foreground="#FF7E7EFF" Height="36">
                <ListBoxItem.InputBindings>
                    <MouseBinding Gesture="LeftClick"  Command="{Binding AddCommand}" />
                </ListBoxItem.InputBindings>
            </ListBoxItem>
        </ListBox>
        <TextBlock x:Name="TimeTB" TextAlignment="Right" Grid.Column="1" Height="29" Margin="0,33,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Foreground="#FF7E7EFF" FontWeight="Bold" FontFamily="Tw Cen MT Condensed Extra Bold" FontSize="26.667" HorizontalAlignment="Right" Width="79"><Run Language="pl-pl" Text="{Binding Path=TimeEllapsed, Converter={StaticResource IntToTimeConverter},Mode=OneWay}"/></TextBlock>
    </Grid>
</Window>
