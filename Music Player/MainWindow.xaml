                                            <DoubleAnimationUsingKeyFrames Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="rectangle1_Copy">
                                                <EasingDoubleKeyFrame KeyTime="0" Value="1"/>
