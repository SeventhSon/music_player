<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netTcpBinding>
                <binding name="NetTcpBinding_ILibraryService" transferMode="Streamed"
                    maxBufferPoolSize="10485760" maxBufferSize="10485760" maxReceivedMessageSize="10485760">
                    <readerQuotas maxDepth="32" maxStringContentLength="10485760"
                        maxArrayLength="10485760" />
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:8523/LibraryServiceLib/"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_ILibraryService"
                contract="LibraryServiceReference.ILibraryService" name="NetTcpBinding_ILibraryService">
                <identity>
                    <dns value="localhost" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>